<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.group8.dao.TravelNotesDao">
    <select id="findTravelNotesById" resultType="LgTravelnotes">
        select * from lg_travelnotes where notes_id=#{notesId}
    </select>

    <select id="findTravelNotesByPraiseNum" resultType="LgTravelnotes">
        SELECT * FROM `lg_travelnotes` order by praise_num DESC
    </select>
    <select id="findLatestTravelNotes" resultType="com.group8.entity.LgTravelnotes">
        select * from lg_scenicspot order by created_time desc
    </select>
</mapper>
